{
    "version": 2,
    "builds": [
          {
            "src": "package.json",
            "use": "@vercel/static-build"
          }
        ],
    "routes": [
          {
            "src": "/(.*)",
            "dest": "/index.html"
          }
        ],
      
    "buildCommand": "npm install react-scripts && npm run build"
  }
  